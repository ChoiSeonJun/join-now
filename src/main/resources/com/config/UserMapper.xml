<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="UserMapper">

 	<select id="idCheck" parameterType="string" 
 	  resultType="User">
 	  select *
 	  from User
 	  where username = #{username}
 	</select>
</mapper>



